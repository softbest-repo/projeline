RewriteEngine On

RewriteRule ^Sitemap\.xml$ geraSitemap.php [L]

ErrorDocument 404 http://localhost/projeline/404/


RewriteRule ger\/$ ger/index.php
RewriteRule ger(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)/(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)/(.+)/(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)/(.+)/(.+)/(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)/(.+)/(.+)/(.+)/(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)/(.+)/(.+)/(.+)/(.+)/(.+)\/$ ger/index.php

RewriteRule \/$ index.php
RewriteRule (.+)\/$ index.php
RewriteRule (.+)/(.+)\/$ index.php
RewriteRule (.+)/(.+)/(.+)\/$ index.php
RewriteRule (.+)/(.+)/(.+)/(.+)\/$ index.php
RewriteRule (.+)/(.+)/(.+)/(.+)/(.+)\/$ index.php
RewriteRule (.+)/(.+)/(.+)/(.+)/(.+)/(.+)\/$ index.php
RewriteRule (.+)/(.+)/(.+)/(.+)/(.+)/(.+)/(.+)\/$ index.php
